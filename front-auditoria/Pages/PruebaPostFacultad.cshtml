@page
@model front_auditoria.Pages.Shared.PruebaPostFacultadModel
@{
}

<div class="container mt-4">
    <h2>Insertar Nueva Facultad</h2>

    <form method="post" class="mt-3">
        <div class="mb-3">
            <label asp-for="facultad.Nombre" class="form-label">Nombre de la Facultad</label>
            <input asp-for="facultad.Nombre" class="form-control" />
            <span asp-validation-for="facultad.Nombre" class="text-danger"></span>
        </div>

        <button type="submit" class="btn btn-primary">Guardar</button>
    </form>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
